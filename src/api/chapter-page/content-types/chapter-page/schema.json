{
  "kind": "collectionType",
  "collectionName": "chapter_pages",
  "info": {
    "singularName": "chapter-page",
    "pluralName": "chapter-pages",
    "displayName": "Chapter-Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "chapter": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::chapter.chapter"
    },
    "history": {
      "type": "richtext"
    },
    "chapter_awards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter-award.chapter-award",
      "mappedBy": "chapter_page"
    },
    "chapter_socials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter-social.chapter-social",
      "mappedBy": "chapter_page"
    },
    "gallery_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
